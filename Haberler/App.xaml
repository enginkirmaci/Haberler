<prism:PrismUnityApplication x:Class="Haberler.App"
                             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                             xmlns:const="using:Haberler.Common.Constants"
                             xmlns:convs="using:Library10.Core.Converters"
                             xmlns:local="using:Haberler"
                             xmlns:prism="using:Prism.Unity.Windows">
    <prism:PrismUnityApplication.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ms-appx:///Themes/Styles.xaml" />
                <ResourceDictionary Source="ms-appx:///Themes/Templates.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <const:ResourceConstants x:Key="ResourceConstants" />
            <convs:RelativeTimeConverter x:Key="RelativeTimeConverter" />
            <convs:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
            <convs:NullBooleanConverter x:Key="NullBooleanConverter" />
            <convs:NullVisibilityConverter x:Key="NullVisibilityConverter" />
            <convs:InvertBooleanConverter x:Key="InvertBooleanConverter" />
        </ResourceDictionary>
    </prism:PrismUnityApplication.Resources>
</prism:PrismUnityApplication>